include file="puretext.filters"

[\w(дп)\w]
hint="гласовне промене: дп -> тп"

[\w(дф)\w]
hint="гласовне промене: дф -> тф"

[\bби\s+(сте|смо)\b]i
hint="спојено: бисте, бисмо"

# End of rules
