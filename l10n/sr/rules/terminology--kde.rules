id="icon"
hint="icon = икона"
valid msgstr="\bикон" !msgstr="\bикони[цч]"
valid msgstr="icon[\w-]*;|\.icon"
valid file="kcmicons.po" msgid="icon theme"
valid file="kcminput.po" msgid="icons on first click"

{\bJava}i
id="java"
hint="Java = јава"
valid msgstr="java[\w-]*;"
valid file="desktop_kdelibs.po" msgid="Javanese"
valid file="kcmkonqhtml.po" msgid="Java(Script)? policy"

{\bJava}
id="java_p"
hint="Java = јаванско"
valid msgstr=""

