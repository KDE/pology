{\bapplet}i
id="applet"
hint="applet = аплет"
valid msgstr="\bаплет"
valid msgstr="applet[\w-]*;"

{\bapplication}i
id="application"
hint="application = програм"
valid !msgstr="\bаплика[цт]"

{\bbrowser}i
id="browser"
hint="browser = прегледач"
valid msgstr="\bпрегледач"
valid msgstr="browser[\w-]*;"

{\bclipboard}i
id="clipboard"
hint="clipboard = клипборд"
valid msgid="\bX Clipboard"
valid msgstr="\bклипборд"

{\bcomputer}i
id="computer"
hint="computer = рачунар"
valid !msgstr="компјутер"

{\bdefault}i
id="default"
hint="default = подразумевано"
valid msgstr="\bподразумева"
valid msgstr="default[\w-]*;"
valid file="kioclient.po" msgid="Mounts the CDROM"
valid file="konsole.po" msgid="DEFAULTENCODING"
valid file="kcmicons.po" msgid="^Default$" msgstr="основна"
valid file="kcmicons.po" msgid="default icon" msgstr="основн\w+ икон"
valid file="kcmkio.po" msgid="KDE's default policy"
valid file="kcmkonqhtml.po" msgid="\.default\w"
valid file="kcmlocale.po" msgid="this country or region" msgstr="локал\w+ станд"
valid file="kcmsmserver.po" msgid="after session exit by default"
valid file="kcmsmserver.po" msgid="happen by default when"
valid file="katepart4.po" msgid="<b>View Defaults</b>"

{\bfolder}i
id="directory"
hint="folder = фасцикла"
valid !msgstr="директориј|омотниц"

{\beditor}i
id="editor"
hint="editor = уређивач"
valid msgstr="\bуређива"
valid msgstr="editor[\w-]*;"
valid msgid="Kate text editor" !msgstr="уредник|едитор"

{\bfile}i
id="file"
hint="file = фајл"
valid !msgstr="\bдатоте"

{\bGNU\b}i
id="gnu"
hint="GNU = Гну"
valid msgstr="\bgnu-\w*;"
valid msgid="GNU Network Object Model Environment"
valid msgstr="gnu[\w-]*;|\.gnu"

{\bGNU\b}
id="gnu_p"
hint="GNU = Гнуово"
valid msgstr=""

{\bhighlight}i
id="highlight"
hint="highlight = истакнути"
