# Terminology checks for KDE translation into Serbian.
# Serbian KDE translation team <kde-i18n-sr@kde.org>
# License: GPLv3

environment kde

{\bapplet}i
id="applet"
hint="applet = аплет"
valid msgstr="\bаплет"
valid msgstr="applet[\w-]*;"

{\bapplication}i
id="application"
hint="application = програм"
valid !msgstr="\bаплика[цт]"

{\bbrowser}i
id="browser"
hint="browser = прегледач"
valid msgstr="\bпрегледач"
valid msgstr="browser[\w-]*;"

{\bclipboard}i
id="clipboard"
hint="clipboard = клипборд"
valid msgstr="\bклипборд"
valid msgstr="clipboard[\w-]*;"

{(?<!backup )\bcop(y|ied)(?!right)}i
id="copy"
hint="copy = копирати"
valid msgstr="\bкопи(ра|ј)"
valid cat="useraccount" msgid="Save copy.*for future" msgstr="Одложи за убудуће"

{\bcopy}i
id="copy_fs"
hint="copy = копирати"
disabled

{\bdriver}i
id="driver"
hint="driver = драјвер"
valid msgstr="\bдрајвер"

{(?<![%</])\bemail}i
id="email"
hint="email = е-пошта"
valid msgstr="\bе-пошт"
valid ctx="^EMAIL OF TRANSLATORS$"
valid msgstr="email[\w-]*;"
valid cat="kabc" msgid="^Preferred Email$" msgstr="адрес"
valid cat="kabc" msgid="^Change Email\.\.\.$" msgstr="адрес"
valid cat="kabc" msgid="^List of Emails$" msgstr="адрес"
valid cat="lokalize" msgid="email.*mailing list" msgstr="адрес"

{\bplugin}i
id="extension"
hint="plugin = прикључак"
disabled

{\bfile}i
id="file"
hint="file = фајл"
valid !msgstr="\bдатоте"

{\bfile[ -]?system(?! browser)}i
id="filesystem"
hint="filesystem|file system = фајл систем"
valid msgstr="\bфајл систем"

{\bGCC}
id="gcc"
hint="GCC = ГЦЦ"
valid msgstr="gcc-*;"

{\bGNU\b}i
id="gnu"
hint="GNU = Гну"
valid msgstr="\bgnu-\w*;"
valid msgid="GNU Network Object Model Environment"
valid msgstr="gnu[\w-]*;|\.gnu"

{\bGNU\b}
id="gnu_p"
hint="GNU = Гнуов"
disabled

{\bicon}i
id="icon"
hint="icon = икона"
valid msgstr="\bикон" !msgstr="\bикони[цч]"
valid msgstr="icon[\w-]*;|\.icon"
valid cat="kcmicons" msgid="icon theme"
valid cat="kcminput" msgid="icons on first click"

{\bJava}i
id="java"
hint="Java = јава"
valid msgstr="java[\w-]*;"
valid cat="desktop_kdelibs" msgid="Javanese"
valid cat="kcmkonqhtml" msgid="Java(Script)? policy"

{\bJava}
id="java_p"
hint="Java = јавански"
disabled

{\bKonqueror}i
id="konqueror"
hint="Konqueror = К-освајач"
valid msgstr="konqueror[\w-]*;|konqueror[./]"
valid cat="kcmkonqhtml" msgstr="<html|<qt"
valid cat="konqueror" msgstr="<img"
valid cat="kdmconfig" msgstr="dolphin[\w-]*;"

{\blabel}i
id="label"
hint="label = етикета"
valid msgstr="\bетикет"
valid msgstr="\bознач"
valid cat="kcmsamba" msgid="Mounted under"

{\bmailing list}i
id="mailinglist"
hint="mailing list = поштанска листа"
valid msgstr="\bпоштанск\w+ лист"

{\bmanager}i
id="manager"
hint="manager = менаџер"
valid msgstr="\bменаџер"
valid msgstr="\bуправ" !msgstr="\bуправ(љач|ник)"
valid msgstr="manager[\w-]*;"
valid cat="filetypes" msgid="Konqueror file manager"

{\bonline}i
id="online"
hint="online = на вези"
valid msgstr="\bна вези"
valid msgstr="online[\w-]*;"

{\bonline}i
id="online2"
hint="online = на вези"
disabled

{\bonline}i
id="online3"
hint="online = на вези"
disabled

{\bpaste[sd]?\b}i
id="paste"
hint="paste = налепити"
valid msgstr="\bналеп"
valid cat="desktop_kdebase" msgid="paste history utility"

{\bplugin}i
id="plugin"
hint="plugin = прикључак"
valid msgstr="\bприкључ[акц]"
valid cat="phonon_gstreamer,kdeqt" msgid="-plugin" msgstr="-plugin"
valid cat="ktraderclient" msgid="KMyApp/Plugin"
valid cat="desktop_kdesdk" msgid="Kate Python Browser Plugin"
valid cat="katefiletemplates" !msgid="^Template Plugin$"

{\bquit(s|ted)?\b}i
id="quit"
hint="quit = напустити"
valid msgstr="\bнапу(шта|сти)"

{\bstring}i
id="string"
hint="string = ниска"
valid !msgstr="знаков\w* низ|низ знаков|слов\w* низ"

{\bthumbnail}i
id="thumbnail"
hint="thumbnail = сличица"
valid msgstr="\bсличиц"

{\btoolbar}i
id="toolbar"
hint="toolbar = трака алатки"
valid msgstr="\bтра[цк]" !msgstr="\bалат[аеиоу]"
valid cat="kdelibs4" msgid="^Show text on toolbar icons *$"
valid cat="kdelibs4" msgid="^Highlight toolbar buttons on mouse over$"
valid cat="kcmstyle" msgid="cursor remains over items in the toolbar"

{\bviewer}i
id="viewer"
hint="viewer = приказивач"
valid msgstr="\bприказивач"
valid msgstr="viewer[\w-]*;"
valid cat="desktop_kdelibs" msgid="text editor.*text viewer and editor"

{\bwizard}i
id="wizard"
hint="wizard = чаробњак"
valid msgstr="\bчаробњак"

{(?<!\.)\bXML\b}i
id="xml"
hint="XML = ИксМЛ"
valid msgstr="xml[\w-]*;"

{\bYahoo}i
id="yahoo"
hint="Yahoo = Јаху"
valid msgstr="yahoo-\w*;"
valid cat="desktop_kdebase" msgid="yahoo%" msgstr="yahoo%"

{\bzoom(?![ -]out)}i
id="zoom"
hint="zoom = увеличати"
valid msgstr="\bувелича"

# End of rules
