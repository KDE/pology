include file="filter.include"

# Remove XML-like tags.
addFilterRegex match="\<\/?\w+\>" on="pmsgid,pmsgstr"

# Remove XML entities.
addFilterRegex match="\&(\w|\.|-)+\;" on="pmsgid,pmsgstr"

addFilterHook name="es:patternAccents" on="pattern"
addFilterHook name="es:patternEntities" on="pattern"


