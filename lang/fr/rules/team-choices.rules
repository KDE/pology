valid cat="knetworkconf"
valid cat="kcmusb"
valid cat="kfileshare"
valid cat="kppp"
valid cat="knetworkconfmodule"
valid cat="kgpg" before="s? chiffré"
valid cat="kgpg" after="déchiffrer les "
