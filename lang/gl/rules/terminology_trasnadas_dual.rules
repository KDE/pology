include file="filter.include"

# «Lapela» traduce a «tab».
[\blapela|\btabula(ción|dor)]i
id="PT-2010-dual-lapela_tabulador_tabulación"
valid msgid="\btab" !before="\border|\bstop" 
hint="«Lapela»,«tabulador» e «tabulación» traducen a «Tab»"

# Variantes de «tab»
[\btabulación]i
id="PT-2010-dual-tabulación"
valid msgid="\btab\b(border|stop)"
valid msgid="\btab"
hint="«tabulación» traduce a «Tab order» e «tab stop»"
 
# «Complemento» traduce a «Plugin»
[complemento]i
id="PT-2010-dual_complemento"
valid msgid="\bplugin"
valid msgid="\bplug.?in"
valid msgid="\badd.?on"
hint="«Complemento» traduce a «Plugin» e a «add-on»"

#«engadido» traduce a «Plugin» e a «added»
[engadido]i
id="PT-2010-dual_engadido"
valid msgid="\bplugin"
valid msgid="\bplug.?in"
valid msgid="added\b"
valid msgid="\badd.?on"
hint="«engadido» traduce a «Plugin», «added» e «addon»"

# «xanela» traduce a «Window»
[\bxanela]i
id="PT-2010-dual_xanela"
valid msgid="\bwindow(\b|s|\s)"
hint="«xanela» traduce a «window»"
 
# «anexar» traduce a «Attach»
[\banex[a-z]{2,}]i
id="PT-2010-dual_anexar"
valid msgid="\battach(\s|ed|s|ing)"
hint="«anexar» traduce a «attach»"

# «anexo» traduce a «Attachment»
[\banexo]i
