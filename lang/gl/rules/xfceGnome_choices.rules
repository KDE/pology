include file="filter.include"


#checks requested by Fran Dieguez

# translator credits
{^translator-credits$}i
id="gnome_translator credits"
hint="comprobar o formato dos creditos da tradución"

# «shell» -> «shell» ou «intérprete de ordes»
{\bshell}i
id="gnome_shell"
valid msgstr="\bshell"
valid msgstr="\binterpretes?\sde\sordes?"
hint="shell -> shell ou interprete de ordes"

# «validation,validity» -> «validación»
{\bvalid(ation|idit(y|ies))}i
id="gnome_validation"
valid msgstr="\bvalidación"
hint="validation -> validación"

# «validación» -> «validaction»
[\bvalidación]i
id="gnome-dual_validation"
valid msgstr="\bvalidation"
hint="validación -> validation"

# «Non se puido» -> «Non foi posíbel»
[\bnon\sse\spuido\b]i
id="expression-gnome_non se puido"
hint="non se puido -> non foi posíbel"

# «houbo un erro» -> «produciuse un erro»
[\bhoubo\sun\s(erro|fallo)\b]i
id="expression-gnome_houbo un erro|fallo"
hint="houbo un erro|fallo -> produciuse un erro|fallo ao"

# «fallo ao» -> «produciuse un fallo ao»
[\bfallo\sao\b]i
id="expression-gnome_fallo ao"
valid after="\bproduciuse\sun\s"
hint="fallo ao -> produciuse un fallo ao"

#substituir tres puntos seguidos por unhas reticencias U2026
[\.{3}]i
id="clGL-gnome_reticencias"
hint="... -> …"

#descarta o uso de «achar» en favor de «encontrar»
[\bach]i
id="clGL-gnome_achar"
hint="achar -> encontrar"

#descarta o uso de «apropiado» en favor de «axeitado»
[\bapropriad]i
id="clGL-gnome_apropiado"
hint="achar -> axeitado"

#descarta o uso de «mudar» en favor de «trocar»
[\bmud(a|o|e)]i
id="clGL-gnome_mudar"
hint="mudar -> trocar"

#descarta o uso de «válido» en favor de «correcto»
[\bvalid(o|a(?!ción))]i
id="clGL-gnome_valido"
hint="valido -> correcto"

#descarta o uso de «predefinido» en favor de «predeterminado»
[\bpredefinid]i
id="clGL-gnome_predefinido"
hint="predefinido -> predeterminado"

#descarta o uso de «aplicación» en favor de «aplicativo»
[\baplicación]i
id="clGL-gnome_aplicación"
hint="aplicación -> aplicativo"

#comprobar caixa de «GNOME»
[\bgnome]i
# valid msgid="\bgnome"
id="clGL-gnome_GNOME"
hint="clGL-all_gnome"

#/end checks requested by Fran Dieguez