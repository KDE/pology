include file="filter.include"

# Remove accelerator and  hyphen inside words.
addFilterRegex match="(\&|\-)" on="pmsgid,pmsgstr"

# «Lapela» traduce a «tab».
[\blapela?|\btabula(ción|dor)]i
id="PT-2010-dual_lapela, tabulador, tabulación->tab"
valid msgid="\btab" !before="\border|\bstop" 
hint="«Lapela»,«tabulador» e «tabulación» traducen a «Tab»"

# Variantes de «tab»
[\btabulación]i
id="PT-2010-dual_tabulación->tab border, tab stop"
valid msgid="\btab\b(border|stop)"
valid msgid="\btab"
hint="«tabulación» traduce a «Tab order» e «tab stop»"
 
# «Complemento» e «engadido» traducen a «Plug.?in»
[complemento?|engadido?]i
id="PT-2010-dual_complemento, engadido->plugin, plug-in"
valid msgid="\bplugin|\bplugin?"
valid msgid="\bplug-in"
hint="«Complemento» ou «engadido» traducen a «Plugin»"

# «xanela» traduce a «Window»
[\bxanela?]i
id="PT-2010-dual_xanela->window"
valid msgid="\bwindow"
hint="«xanela» traduce a «window»"
 
# «anexar» traduce a «Attach»
[\banex[a-z]+]i
id="PT-2010-dual_anexar->attach"
valid msgid="\battach(\"\"|ed|s)"
hint="«anexar» traduce a «attach»"

# «anexo» traduce a «Attachment»
[\banexo?]i
id="PT-2010-dual_anexo->attachment"
valid msgid="attachment?"
hint="«anexo» traduce a «attachment»"

# «orde» traduce a «Command»
[\borde(\"\"|s)]i
id="PT-2010-dual_orde->command"
valid msgid="\bcommand"
hint="«orde» traduce a «command»"

# «invalid» tradúcese como «incorrecto»/«non é válido»
[\bincorrecto|(non\s(é\s|"")válid(o|a))|(non\sson\sválid(o|a)s)]i
valid !before="argumento"
id="noPT-2010-dual_incorrecto, non valido->invalid"
valid msgid="\binvalid"
hint="«incorrecto»/«non é válido» traducen a «invalid»"

# «inaceptábel» traduce a «ilegal»
[\binaceptábe(l|is)\b]i
id="PT-2010-dual_inaceptábel->ilegal"
valid msgid="\bilegal"
hint="«inaceptábel» traduce a «ilegal»"

# «grella» traduce a «grid»
[\grella?]i
id="noPT-2010-dual_grid->grella"
valid msgid="\bgrid?"
hint="«grella» traduce a «grid»"

# «mostrar» traduce a «show»
[\bmostr?]i
#valid msgid="\b[sS]how(\"\"|s|ed|n)\b"
valid msgid="\bshow"
valid msgid="\bdisplay"
id="noPT-2010-dual_mostrar->show, display"
hint="«mostrar» traduce a «show» e «display»"

# «acochar» traduce a «hide»
[\bacoch]i
valid msgid="\bhide(\b|s|ed)"
id="noPT-2010-dual_acochar->hide"
hint="«acochar» traduce a «hide»"

# «inserir» ou «introducir» traducen a «insert»
[\bins(e|i)r|\bintroduc]i
id="noPT-2010-dual_inserir, introducir->insert, enter"
valid msgid="\binsert|\benter"
hint="«inserir» ou «introducir» traducen a «insert» ou «enter»"

# «buscar»/«busca» traducen a «search»
[\bbusc]i
valid msgid="\bsearch?"
valid msgid="\bfind|\bfound"
valid msgid="\blocate"
id="PT-2010-dual_buscar->search, find, locate"
hint="«busca(r)» traduce a «search», «find» e «locate»"

# «atopar» traducen a «find»
[\batop]i
valid msgid="\bfind|\bfound"
valid msgid="\blocate"
id="noPT-2010-dual_atopar-> find, locate"
hint="«atopar» traduce a «find» e «locate»"

# «situar» traducen a «locate» ou «place»
[\bsitu]i
valid msgid="\blocate|\bplace"
id="PT-2010-dual_situar->locate,place"
hint="«situar» traduce a «to locate» ou «to place»"
