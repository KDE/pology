# «abort» tradúcese como «interromper»
{\babort(ed|ing|s)?\b}i
id="PT-2011_abort"
valid msgstr="\binterr([oó]mp[aáeéoó][^\W\d_]*|ompid[ao]s?|upcións?)\b"
hint="«abort» tradúcese como «interromper»"

# «interromper» traduce a «abort»
[\binterr([oó]mp[aáeéoó][^\W\d_]*|ompid[ao]s?|upcións?)\b]i
id="noPT-2011-dual_abort"
valid msgid="\babort(ed|ing|s)?\b"
valid msgid="\bbr(eak(ing|s)?|roken?)\b"
valid msgid="\binterrupt(ed|ing|ions?|s)?\b"
