include file="filter.include"

# «about» tradúcese como «sobre»
{\b(?<!^)about}i
valid before="\sto\b"
valid after="\bknow(s|\"\")\s"
valid after="\bforgets?\s"
id="PT-2011_about"
valid env="kde"
valid msgstr="\bsobre\b"
hint="«about» tradúcese como «sobre»"

# «abort» tradúcese como «interromper»
{\babort}i
id="PT-2011_abort"
valid msgstr="interr(o|ó)mp"
hint="«abort» tradúcese como «interromper»"

# «interromper» traduce a «abort»
[\binterromp]i
id="noPT-2011-dual_abort"
valid msgid="\babort"
valid msgid="\binterrupt"
valid msgid="\bbreak" env="kde"
hint="«interromper» traduce a «abort» (e a «interrupt») (e a «break»)"

# «applet» tradúcese como «applet»
{\bapplet}i
id="PT-2011_applet"
valid msgstr="\bapplet"
hint="«applet» tradúcese como «applet»"
valid !env="kde"

# «applet» traduce a «applet»
[\bapplet]i
id="PT-2011-dual_applet"
valid msgid="\bapplet"
hint="«applet» traduce a «applet»"
valid !env="kde"

# «application» tradúcese como «aplicativo»
{\bapplication}i
valid after="</?" before=">" env="xfce"
valid before="/x-executable"
valid before="/x-shellscript"
valid before="/x-desktop"
valid after="Type="
id="PT-2011_application"
