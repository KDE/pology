# Remove XML entities.
addFilterHook name="remove/remove-paired-ents-tick~'~'" on="msg"

# Remove some abreviations.
addFilterRegex match="\b(admin|cartr|cert|dec|Dr|etc|expr|hex|id|inc|lab|Mr|no|obs|oct|org|seq|sync|torr)\." repl="~" on="pmsgid"

# Falta espacio al final del párrafo.
{\s$}i
valid msgstr="\s$"
hint="Compruebe el uso de espacio al final del párrafo (falta en la traducción)."

# Falta espacio antes del fin de línea «\n».
{\s\\n}i
valid msgstr="\s\\n"
hint="Compruebe el uso de espacio antes del fin de línea «\n» (falta en la traducción)."

# Falta espacio antes del fin de párrafo.
{\s\<\/p\>}i
valid msgstr="\s\<\/p\>"
hint="Compruebe el uso de espacio antes del fin de párrafo «</p>» (falta en la traducción)."

# Punto al final del párrafo.
{\.\s*$}i
valid msgstr="[\.\!\?]\s*$"
hint="Compruebe el uso de punto al final del párrafo (falta en la traducción)."

# Punto en el fin de línea «\n».
{\.\s*\\n}i
valid msgstr="[\.\!\?]\s*\\n"
hint="Compruebe el uso de punto antes del fin de línea «\n» (falta en la traducción)."

# Punto en el fin de párrafo.
{\.\s*\<\/p\>}i
valid msgstr="[\.\!\?]\s*\<\/p\>"
hint="Compruebe el uso de punto antes del fin de párrafo «</p>» (falta en la traduccción)."

# Dos puntos al final del párrafo.
{\:\s*$}i
valid msgstr="\:\s*$"
hint="Compruebe el uso de dos puntos al final del párrafo (faltan en la traducción)."

# Dos puntos en el fin de línea «\n».
{\:\s*\\n}i
valid msgstr="\:\s*\\n"
hint="Compruebe el uso de dos puntos antes del fin de línea «\n» (faltan en la traducción)."

# Dos puntos al final del párrafo.
{\:\s*\<\/p\>}i
valid msgstr="\:\s*\<\/p\>"
hint="Compruebe el uso de dos puntos al final del párrafo «</p>» (faltan en la traducción)."

# Coma al final del párrafo.
{\,\s*$}i
valid msgstr="\,\s*$"
hint="Compruebe el uso de coma al final del párrafo (falta en la traducción))."

# Coma en el fin de línea «\n».
{\,\s*\\n}i
valid msgstr="\,\s*\\n"
hint="Compruebe el uso de coma antes del fin de línea «\n» (falta en la traducción)"

# Coma al final del párrafo.
{\,\s*\<\/p\>}i
valid msgstr="\,\s*\<\/p\>"
hint="Compruebe el uso de coma al final del párrafo «</p>» (falta en la traducción)"

# Punto y coma al final del párrafo.
{\;\s*$}i
valid msgstr="\;\s*$"
hint="Compruebe el uso de punto y coma al final del párrafo (falta en la traducción."

# Punto y coma en el fin de línea «\n».
{\;\s*\\n}i
valid msgstr="\;\s*\\n"
hint="Compruebe el uso de punto y coma antes del fin de línea «\n» (falta en la traducción)."

# Punto y coma al final del párrafo.
{\;\s*\<\/p\>}i
valid msgstr="\;\s*\<\/p\>"
hint="Compruebe el uso de punto y coma al final del párrafo «</p>» (falta en la traducción)."

# Paréntesis al final del párrafo.
{\.\)\s*$}i
valid msgstr="\)\.\s*$"
valid msgstr="etc\.\)s*$"
valid msgstr="\w\.$"
valid msgstr="\.\.\.\)$"
hint="Compruebe el orden del paréntesis y punto al final del párrafo (al revés que en inglés)."

# Paréntesis en el fin de línea «\n».
{\.\)\s*\\n}i
