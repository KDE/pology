# Omit group of words not translated
addFilterRegex match="(?u)(echo\snest|make\s(un)?install|(little|big)\sendian|summer\sof\scode|if\=|of\=)" repl="~" on="pmsgstr"
addFilterRegex match="(?u)(Planet\sKDE|KDE\sApps|K\sDesktop\sEnvironment|Qt\sDesigner)" repl="~" on="pmsgstr"
addFilterRegex match="(?u)(Microsoft\sWindows|Internet\sExplorer|Outlook\sExpress)" repl="~" on="pmsgstr"
addFilterRegex match="(?u)(Mozilla\sFirefox|Google\sChrome|Pegasus\sMail)" repl="~" on="pmsgstr"

# Omit words between quotes.
addFilterRegex match="(?u)«\w+»" repl="~" on="pmsgstr"

# Omit words between brackets.
addFilterRegex match="(?u)\{\w+\}" repl="~" on="pmsgstr"
addFilterRegex match="(?u)\(\w+\)" repl="~" on="pmsgstr"

# Omit compounds words.
addFilterRegex match="(?u)\b\w+(\-\w+)+" repl="~" on="pmsgstr"

# Omit words preceded by some special characters.
addFilterRegex match="(?u)(\/|\-|\+|\:|\.)\w+" repl="~" on="pmsgstr"

# Omit words witch contains at least a capital letter.
addFilterRegex match="(?u)\b[a-z]+[A-Z]\w*" casesens="yes" repl="~" on="pmsgstr"
addFilterRegex match="(?u)\b[A-Z]\w*?[A-Z]\w*" casesens="yes" repl="~" on="pmsgstr"
addFilterRegex match="(?u)\w+\s+[A-Z]\w*" casesens="yes" repl="~" on="pmsgstr"

# Omit words witch contains at least one number.
addFilterRegex match="(?u)\b\w+?\d\w*" repl="~" on="pmsgstr"

*hook name="spell/check-spell-sp~lang='es'" on="msgstr"
id="sp-0-check-spell-hook"
hint="Error ortográfico."

# End of rule file
