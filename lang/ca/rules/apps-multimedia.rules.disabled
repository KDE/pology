# - multimèdia -

# Remove XML entities.
include file="filter.include"
# addFilterRegex match="\\" repl="/" on="pmsgid"
# addFilterRegex match="&.*;" repl="" on="pmsgid"
# addFilterRegex match=">\s<" repl="" on="pmsgid"
# addFilterRegex match=">\s\s<" repl="" on="pmsgid"
# addFilterRegex match="<.*>" repl="" on="pmsgid"
# addFilterRegex match="-" repl=" " on="pmsgid,pmsgstr" # KDE
# addFilterRegex match="_" repl="" on="pmsgid,pmsgstr" # Gnome
# addFilterRegex match="@@image: 'images/.*'" repl="" casesens="yes" on="pmsgid" # Gnome
# addFilterRegex match="-.*-" repl="-" on="pmsgid" # Gnome

# Remove accelerators inside words.
addFilterRegex match="&" on="pmsgid,pmsgstr"

