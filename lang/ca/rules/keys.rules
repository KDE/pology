# Tab -> Tabulador
{(&Tab;|<key(cap|sym)>.?Tab.?</key(cap|sym)>|TAB\skey)}
valid msgstr="&Tab;" # <keycap>Tabulador</keycap>
valid msgstr="<keycap>Tabulador</keycap>"
valid msgstr="tecla &Tab;" # a la doc
valid msgstr="tecla Tab" # a l'IGU
hint="Es tradueix per «&Tab;, tecla &Tab;, <keycap>Tabulador</keycap>» (keys)"

# Arrow keys
# Page Down -> Av. Pàg.
{(<key(cap|sym)>).?(Page.?Down|PgDown).?(</key(cap|sym))>}
valid msgstr="<keycap>Av. Pàg.</keycap>"
valid msgstr="AvPàg" # A l'IGU
hint="Es tradueix per «AvPàg (a l'IGU), <keycap>Av. Pàg.</keycap> (a la DOC)» (keys)"

# Page Up -> Re. Pàg.
{(<key(cap|sym)>).?(Page.?Up|PgUp).?(</key(cap|sym))>}
valid msgstr="<keycap>Re. Pàg.</keycap>"
valid msgstr="RePàg" # A l'IGU
hint="Es tradueix per «RePàg (a l'IGU), <keycap>Re. Pàg.</keycap> (a la DOC)» (keys)"

# Cursor keys
