valid msgstr="Cursor <keysym>Avall</keysym>/<keysym>Dret</keysym>" cat="kipi-plugins_imageviewer" # docmessages
valid msgstr="fletxa avall" # A l'IGU
hint="Es tradueix per «fletxa avall (a l'IGU), <keysym>Cursor avall</keysym>|<keysym>Amunt</keysym> i <keysym>Avall</keysym> (a la DOC)» (keys)"

