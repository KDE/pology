include file="filter.include"

# «tea» es tradueix per «te» (sense accent).
{\btea\b}i
valid msgstr="te"
hint="«tea» es tradueix per «te» (sense accent)"

# «té» del verb tenir cal que porti accent.
{\bte\b}i
valid msgid="tea"
valid msgid="te"
hint="«té» (verb tenir) cal que porti accent"

# «world» es tradueix per «món» (amb accent).
{\bworld\b}i
valid msgstr="mapamundi"
valid msgstr="món"
valid msgstr="mundial"
valid msgstr="web"
valid msgstr="world"
valid msgstr="altres" cat="kfileshare"
valid msgstr="tothom" cat="kfileshare"
valid msgstr="tots" cat="kdeprint_theory" #docmessage
valid msgstr="WWW" before="\swide\sweb"
hint="«world» es tradueix per «món» (amb accent)"

# End of common mistakes rules file
