addFilterRegex match="&" on="pmsgid,pmsgstr"

{^\bFinds?}i
valid msgid="Find\s(an\sObject|City|in\sMessage|History|Messages|PDF|provider\ssettings|Resources|someone|Text)"
valid msgstr="Troba les referències" # applications/lspclient
valid msgstr="Trobar? (l')?objectes?" # extragear-edu/kstars
valid msgstr="Troba les vores" # extragear-graphics/digikam
valid msgstr="Troba" cat="gcompris_qt,gcompris_voices,kdeconnect-android,kdeconnect-kde._json_,org.kde.heaptrack.appdata,plasma_runner_kdeconnect_findmyphone"
valid !msgstr="Troba"
valid msgstr="Cerca"
hint="Cal substituir-ho per «Cerca» (revisió)"

{>\bFinds?}i
valid msgstr=">Troba l'objecte" # extragear-edu/kstars
valid msgstr=">Troba les vores" # extragear-graphics/digikam
valid !msgstr=">Troba"
valid msgstr=">Cerca"
hint="Cal substituir-ho per «Cerca» (revisió)"

{\"\bFinds?}i
