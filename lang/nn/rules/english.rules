# Frequently-used English words that should be replaced by Norwegian Nynorsk words.
# Karl Ove Hufthammer <karl@huftis.org>
# License: GPLv3

include file="puretext.filters"

[printer]i
id="printer"
hint="printer → skrivar"
valid after="system-config-"

[\bserv[ae]r(en|ar|ane)?\b]i
id="server"
hint="server → tenar"

{socket}i
id="socket"
hint="socket → socket (ikkje «sokkel» e.l.)"
valid msgstr="socket"

[(socket\w)|(\wsocket)]i
id="socketord"
hint="«socket» skal ha bindestrek ved bøying og samansettingar («socket-en», ikkje «socketen», og «socket-feil», ikkje «socketfeil»)"

[\bURL-]i
hint="URL → (nett)adresse (med nokre unntak)"
